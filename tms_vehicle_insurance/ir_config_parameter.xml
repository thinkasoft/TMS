<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">


	<!-- Parameter to Avoid dispatching Travels when Vehicle Insurance Policy expires on next [parameter] days -->
	<record model="ir.config_parameter" id="tms_vehicle_insurance_security_days">               
      <field name="key">tms_vehicle_insurance_security_days</field> 
      <field name="value">5</field>              
    </record>

	<!-- Parameter to define notification for sending mails for Vehicle Insurance Policies about to expire on next [parameter] days -->
	<record model="ir.config_parameter" id="tms_vehicle_insurance_notification_x_days">               
      <field name="key">tms_vehicle_insurance_notification_x_days</field> 
      <field name="value">30</field>              
    </record>
        
    </data>
</openerp>
